#include<stdio.h>

int main(){

double c, f;

printf("\nDigite a temperatura em Fahrenheit: ");
scanf("%lf", &f);

c = (f - 32.0) * (5.0 / 9.0);

printf("\nA temperatura em Celsius é: %lf ", c);

return 0;
}

/* Execução:
   Valor a ser digitado: 565
   Resultado final: 296.111111 */
